importar 'consola' como consola;
importar 'ws' como ws;
const conexion = ws();
fn abrir() {
  consola.pintar('Conectado');
  conexion.enviar('ping');
};
fn mensaje(datos) {
  consola.pintar(datos);
  conexion.desconectar();
};
fn cerrar() {
  consola.pintar('Desconectado');
};
conexion.en('abrir', abrir);
conexion.en('mensaje', mensaje);
conexion.en('cerrar', cerrar);
conexion.en('error', fn(error) {
  consola.pintar('Error con el socket',error);
});
conexion.conectar('ws://localhost:8000');
