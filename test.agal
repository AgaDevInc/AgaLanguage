const http = requiere('http');
const ListaEnteros = requiere('ListaEnteros');

http.servidor(80, server, err)

fn server(req){
  const agent = req.cabeceras['user-agent']
  ret http.Respuesta(ListaEnteros.desde('Tu agente es:\n\n' + agent))
}
fn err(error){
  const msg = 'Error en el servidor: ' + error.mensaje
  ret http.Respuesta(ListaEnteros.desde(msg))
}